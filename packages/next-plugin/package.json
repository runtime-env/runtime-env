{
  "name": "@runtime-env/next-plugin",
  "version": "0.1.0",
  "description": "Opinionated Next.js plugin for runtime-env",
  "license": "MIT",
  "author": "Ernest",
  "keywords": [
    "nextjs",
    "plugin",
    "runtime-env"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "pack": "npm pack && node ../../scripts/rename-tgz.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runtime-env/runtime-env.git",
    "directory": "packages/next-plugin"
  },
  "bugs": {
    "url": "https://github.com/runtime-env/runtime-env/issues"
  },
  "homepage": "https://github.com/runtime-env/runtime-env#readme",
  "peerDependencies": {
    "@runtime-env/cli": "*",
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@runtime-env/cli": "^1.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "next": "^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chokidar": "5.0.0"
  },
  "type": "module"
}
