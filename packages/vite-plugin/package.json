{
  "name": "@runtime-env/vite-plugin",
  "version": "0.1.3",
  "description": "Opinionated Vite plugin for runtime-env",
  "license": "MIT",
  "author": "Ernest",
  "keywords": [
    "vite",
    "plugin",
    "runtime-env"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
