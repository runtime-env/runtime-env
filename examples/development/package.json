  "devDependencies": {
    "@runtime-env/cli": "0.7.3",
    "concurrently": "9.2.0",
    "html-webpack-plugin": "5.6.3",
    "webpack": "5.100.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  }
}
