{
  "name": "development",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prepare": "echo FOO=\"development\" > .env",
    "serve": "concurrently --kill-others \"npm run serve:env\" \"npm run serve:webpack\"",
    "serve:env": "runtime-env --watch gen-js --env-file .env --output-file public/runtime-env.js",
    "serve:webpack": "webpack serve --mode development",
    "test": "start-server-and-test 'serve' 8080 'cypress run'"
  },
  "devDependencies": {
    "concurrently": "9.2.1",
    "cypress": "15.10.0",
    "html-webpack-plugin": "5.6.6",
    "start-server-and-test": "2.1.3",
    "webpack": "5.105.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3"
  }
}
